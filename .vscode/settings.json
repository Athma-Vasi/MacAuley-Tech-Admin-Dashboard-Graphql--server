{
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.addMissingImports.ts": "explicit",
        "source.organizeImports": "explicit",
        "source.removeUnusedImports.ts": "explicit"
    },
    "deno.enable": true,
    "deno.lint": true
}
